{
  "name": "bdd-wizard",
  "private": true,
  "sideEffects": false,
  "packageManager": "pnpm@7.33.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "run-s build:*",
    "build:remix": "remix build --sourcemap",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "remix dev -c \"node ./server/dev-server.js\" --no-restart",
    "deploy": "fly deploy --remote-only",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test": "vitest",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:gen": "playwright codegen",
    "pretest:e2e:run": "pnpm build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "playwright install --with-deps chromium",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
  "dependencies": {
    "@remix-run/express": "^1.17.0",
    "@remix-run/node": "^1.17.0",
    "@remix-run/react": "^1.17.0",
    "@remix-run/router": "^1.6.3",
    "@remix-run/server-runtime": "^1.17.0",
    "address": "^1.2.2",
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "close-with-grace": "^1.2.0",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.1.4",
    "execa": "^7.1.1",
    "express": "^4.18.2",
    "get-port": "^7.0.0",
    "helmet": "^7.0.0",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@flydotio/dockerfile": "^0.2.14",
    "@playwright/test": "^1.35.0",
    "@remix-run/dev": "^1.17.0",
    "@remix-run/eslint-config": "^1.17.0",
    "@remix-run/serve": "^1.17.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.40.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "esbuild": "^0.18.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.7",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.22",
    "prettier": "2.8.7",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.30.1"
  },
  "engines": {
    "node": "18"
  }
}
